@{
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>ArcGIS API for JavaScript Tutorials: Display point, line, and polygon graphics</title>

    <link rel="stylesheet" href="https://js.arcgis.com/4.16/esri/themes/light/main.css">
    <script src="https://js.arcgis.com/4.16/"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script>
        require([
            "esri/Map",
            "esri/views/MapView",
            "esri/Graphic",
            "esri/layers/GraphicsLayer"
        ], function (Map, MapView, Graphic, GraphicsLayer) {
                $(document).ready(function () {
                    var map = new Map({
                basemap: "streets-relief-vector"
            });

            var view = new MapView({
                container: "viewDiv",
                map: map,
                center: [105.843330, 21.022330],
                zoom: 7
            });

            var graphicsLayer = new GraphicsLayer();
                map.add(graphicsLayer);

            var arrPoint = new Array();
            function IntervalFunc() {
                 $.ajax({
                        type: 'GET',
                        url: '@Url.Action("LoadData")',
                        dataType: 'json',
                        data: { id: '' },
                        success: function (data) {
                            //graphicsLayer.removeAll();
                            var items = '';
                            $.each(data, function (i, item) {

                                // Create a point
                                var point = {
                                    type: "point",
                                    longitude: item.Longitude,
                                    latitude: item.Latitude
                                };

                                var simpleMarkerSymbol = {
                                    type: "picture-marker",
                                    url: "data:image/png;base64,",
                                    width: "20px",
                                    height: "20px",
                                    angle: item.Track
                                };


                                var attributes = {
                                    Name: "My point", // The name of the
                                    Lon: item.Longitude,
                                    Lat: item.Latitude// The owner of the
                                };
                                // Create popup template
                                var popupTemplate = {
                                    title: "{Name}",
                                    content: "Longitute <b>{Lon}</b>. Latitude <b>{Lat}</b>"
                                };
                                var pointGraphic = new Graphic({
                                    geometry: point,
                                    symbol: simpleMarkerSymbol,
                                    attributes: attributes,
                                    popupTemplate: popupTemplate
                                });
                                arrPoint.push(point);
                                graphicsLayer.add(pointGraphic);

                            });
                        },
                        error: function (ex) {
                            var r = jQuery.parseJSON(response.responseText);
                            //alert("Message: " + r.Message);
                            console.log(r.Message);
                        }
                    });
            }

                function stopIntervalFunc() { // call this to stop your interval.
                    graphicsLayer.removeAll();
                clearInterval(id);

                //graphicsLayer.removeMany(arrPoint);

            }
            var id = '';
                $('#btnLoad').click(function () {
                    //alert(true);
                id = setInterval(IntervalFunc, 15000); //call test every 10 seconds.
            });
                $('#btnStop').click(function () {
                    stopIntervalFunc();
            });
                });

            // Create a point
        });</script>
    <style>
        html, body {
            padding: 0;
            margin: 0;
            height: 98%;
            width: 100%;
        }

        .main-padding {
            padding: 15px;
        }

        .viewDiv {
            height: 98%;
        }

        .title {
            background-color: #343a40;
            height: 70px;
            text-align: center;
            color: #fff;
            font-size: 25px;
            padding-top: 17px;
            text-transform: uppercase;
            margin-bottom : 0;
        }
        #viewDiv {
            height: 100%;
            width: 100%;
            padding: 0;
            margin: 0;
        }
        .mainMap {
            height : calc(100% - 60px);
            display : block;
        }
    </style>
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <p class="title">Flight Map</p>
        </div>
    </div>
    
    <div class="main-padding">

        <div class="row">
            <div class="col-md-9">
                <div class="mainMap">
                    <div id="viewDiv"></div>
                </div>
            </div>
            <div class="col-md-3">
                <button id="btnLoad">Load Data</button>
                <br />
                <button id="btnStop">Stop Display data</button>
            </div>
        </div>
    </div>


</body>
</html> 