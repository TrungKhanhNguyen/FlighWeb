
@{
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>ArcGIS API for JavaScript Tutorials: Display point, line, and polygon graphics</title>

    <link rel="stylesheet" href="https://js.arcgis.com/4.16/esri/themes/light/main.css">
    <script src="https://js.arcgis.com/4.16/"></script>
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" />*@
    <link href="~/Content/font-awesome.css" rel="stylesheet" />

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/mainesri.css" rel="stylesheet" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.1/themes/smoothness/jquery-ui.min.css">
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

    <script>
        require([
            "esri/Map",
            "esri/views/MapView",
            "esri/Graphic",
            "esri/layers/GraphicsLayer"
        ], function (Map, MapView, Graphic, GraphicsLayer) {
            $(document).ready(function () {
                    var map = new Map({
                basemap: "streets-relief-vector"
            });

            var view = new MapView({
                container: "viewDiv",
                map: map,
                center: [105.843330, 21.022330],
                zoom: 7
            });

            var graphicsLayer = new GraphicsLayer();
                map.add(graphicsLayer);

                var arrPoint = new Array();

                var id = '';
            function IntervalFunc() {
                 $.ajax({
                        type: 'GET',
                        url: '@Url.Action("LoadData")',
                        dataType: 'json',
                        data: { id: '' },
                        success: function (data) {
                            graphicsLayer.removeAll();
                            var items = '';
                            $.each(data, function (i, item) {

                                // Create a point
                                var point = {
                                    type: "point",
                                    longitude: item.Longitude,
                                    latitude: item.Latitude
                                };

                                var simpleMarkerSymbol = {
                                    type: "picture-marker",
                                    url: "data:image/png;base64,",
                                    width: "15px",
                                    height: "15px",
                                    angle: item.Track
                                };

                                var datebysecond = item.DateGenerate.replace("/Date(", "").replace(")/", "");
                                var d = new Date();
                                d.setTime(datebysecond);
                                var dateString =
                                    d.getUTCFullYear() + "-" +
                                    ("0" + (d.getMonth() + 1)).slice(-2) + "-" +
                                    ("0" + d.getDay()).slice(-2) + " " +
                                    ("0" + d.getHours()).slice(-2) + ":" +
                                    ("0" + d.getMinutes()).slice(-2) + ":" +
                                    ("0" + d.getSeconds()).slice(-2);
                                //alert(item.DateGenerate);

                                var attributes = {
                                    Lon: item.Longitude,
                                    Lat: item.Latitude,// The owner of the
                                    Icao: item.ICAO,
                                    Callsign: item.Callsign,
                                    Altitude: item.Altitude,
                                    Speed: item.Speed,
                                    Track: item.Track,
                                    VerticalRate: item.VerticalRate,
                                    DateStr: dateString
                                };
                                // Create popup template
                                var popupTemplate = {
                                    title: "ICAO: <b>{Icao}</b>",
                                    content: "<b>{DateStr}</b>. Callsign <b>{Callsign}</b><br />."
                                        + "Kinh độ <b>{Lon}</b>. Vĩ độ <b>{Lat}</b>. Tốc độ <b>{Speed}</b>. Hướng <b>{Track}</b>."
                                };

                                var pointGraphic = new Graphic({
                                    geometry: point,
                                    symbol: simpleMarkerSymbol,
                                    attributes: attributes,
                                    popupTemplate: popupTemplate
                                });
                                arrPoint.push(point);
                                graphicsLayer.add(pointGraphic);


                            });
                        },
                        error: function (ex) {
                            var r = jQuery.parseJSON(response.responseText);
                            //alert("Message: " + r.Message);
                            console.log(r.Message);
                        }
                    });
            }

            function stopIntervalFunc() { // call this to stop your interval.
                //graphicsLayer.removeAll();
                clearInterval(id);
                }
                var number = document.getElementById('counting');
                $('#loadTime').on("change paste keyup", function () {
                    number.innerText = $(this).val();
                });



                var staticCount = $('#loadTime').val();
                var counting = staticCount;
                var countFunc = '';
                function start() {
                    countFunc = setInterval(increase, 1000);
                }

                function increase() {
                    if (counting > 0) {
                        counting--;
                        number.innerText = counting;
                    } else if (counting == 0) {
                        counting = staticCount;
                    }

                }
                function resetCounting() {
                    clearInterval(countFunc);
                    counting = staticCount;
                    number.innerText = counting;
                }

                $('#btnLoad:not(.disabled)').click(function () {
                    if (!$(this).hasClass('disabled')) {
                        graphicsLayer.removeAll();
                        var ts = $('#loadTime').val() * 1000;
                        id = setInterval(IntervalFunc, ts); //call test every 10 seconds.
                        $(this).addClass('disabled');
                        $('#btnStop').removeClass('disabled');
                    }
                    start();
            });
                $('#btnStop').click(function () {
                    if (!$(this).hasClass('disabled')) {
                        stopIntervalFunc();
                        $(this).addClass("disabled");
                        $('#btnLoad').removeClass('disabled');
                    }
                    resetCounting();
                });

                

                $('.btnSearch').click(function () {
                    stopIntervalFunc();
                    $('#btnLoad').removeClass('disabled');
                    $('#btnStop').addClass('disabled');
                    
                    var icao = $('#inputICAO').val();
                    var fromdate = $('#datepicker1').val();
                    var todate = $('#datepicker2').val();
                    var fromtime = $('.time1').val();
                    var totime = $('.time2').val();
                    $.ajax({
                        type: 'GET',
                        url: '@Url.Action("SearchData")',
                        dataType: 'json',
                        data: { inputICAO: icao, datepicker1: fromdate, datepicker2: todate, fromtime: fromtime, totime : totime },
                        success: function (data) {
                            //graphicsLayer.removeAll();
                            var items = '';
                            var listPath = new Array(data.length);
                            $('.table-map tbody').empty();
                            $.each(data, function (i, item) {

                               
                                var datebysecond = item.DateGenerate.replace("/Date(", "").replace(")/", "");
                                var d = new Date();
                                d.setTime(datebysecond);
                                var dateString =
                                    d.getUTCFullYear() + "-" +
                                    ("0" + (d.getMonth() + 1)).slice(-2) + "-" +
                                    ("0" + d.getDate()).slice(-2) + " " +
                                    ("0" + d.getHours()).slice(-2) + ":" +
                                    ("0" + d.getMinutes()).slice(-2) + ":" +
                                    ("0" + d.getSeconds()).slice(-2);
                                
                                var html = "<tr><td><span class='dateString'>" + dateString + "</span></td>" +
                                    "<td><span class='ICAO'>" + item.ICAO + "</span></td>" +
                                    "<td><span class='Callsign'>" + item.Callsign + "</span></td>" +
                                    "<td><span class='Longitude'>" + item.Longitude + "</span> / <span class='Latitude'>" + item.Latitude + "</span></td>" +
                                    "<td><span class='Speed'>" + item.Speed + "</span></td>" +
                                    "<td><span class='Altitude'>" + item.Altitude + "</span> / <span class='Track'>" + item.Track + "</span></td>" +
                                    "<td><span class='VerticalRate'>" + item.VerticalRate + "</span></td>" +
                                    "</tr>"

                                $('.table-map tbody').append(html);
                                //// Create a point
                                //var point = {
                                //    type: "point",
                                //    longitude: item.Longitude,
                                //    latitude: item.Latitude
                                //};

                                //var simpleMarkerSymbol = {
                                //    type: "picture-marker",
                                //    url: "data:image/png;base64,",
                                //    width: "20px",
                                //    height: "20px",
                                //    angle: item.Track
                                //};
                                //var attributes = {
                                //    Lon: item.Longitude,
                                //    Lat: item.Latitude,// The owner of the
                                //    Icao: item.ICAO,
                                //    Callsign: item.Callsign,
                                //    Altitude: item.Altitude,
                                //    Speed: item.Speed,
                                //    Track: item.Track,
                                //    VerticalRate: item.VerticalRate,
                                //    DateStr: dateString
                                //};
                                //// Create popup template
                                //var popupTemplate = {
                                //    title: "ICAO: <b>{Icao}</b>",
                                //    content: "<b>{DateStr}</b>. Kinh độ <b>{Lon}</b>. Vĩ độ <b>{Lat}</b>. Cao độ <b>{Altitude}</b>.<br />Callsign <b>{Callsign}</b>."
                                //        + " Tốc độ <b>{Speed}</b>. Track <b>{Track}</b>. VerticalRate <b>{VerticalRate}</b>."
                                //};
                                //var pointGraphic = new Graphic({
                                //    geometry: point,
                                //    symbol: simpleMarkerSymbol,
                                //    attributes: attributes,
                                //    popupTemplate: popupTemplate
                                //});


                                //listPath[i] = [item.Longitude, item.Latitude];
                                //graphicsLayer.add(pointGraphic);

                            });

                            $('#btnShowMap').click(function () {
                                //stopIntervalFunc();
                                graphicsLayer.removeAll();
                                $('.table-map tbody tr').each(function (i) {
                                    var lon = $(this).find('.Longitude').text();
                                    var lat = $(this).find('.Latitude').text();
                                    var alti = $(this).find('.Altitude').text();
                                    var icao = $(this).find('.Icao').text();
                                    var speed = $(this).find('.Speed').text();
                                    var track = $(this).find('.Track').text();
                                    var dateString = $(this).find('.dateString').text();
                                    var callsign = $(this).find('.Callsign').text();
                                    var vertical = $(this).find('.VerticalRate').text();


                                    var point = {
                                    type: "point",
                                    longitude: lon,
                                    latitude: lat
                                    };

                                var simpleMarkerSymbol = {
                                    type: "picture-marker",
                                    url: "data:image/svg+xml;base64,",
                                    width: "15px",
                                    height: "15px",
                                    angle: track
                                };

                                    var attributes = {
                                        Lon: lon,
                                        Lat: lat,// The owner of the
                                        Icao: icao,
                                        Callsign: callsign,
                                        Altitude: alti,
                                        Speed: speed,
                                        Track: track,
                                        VerticalRate: vertical,
                                        DateStr: dateString
                                    };
                                    // Create popup template
                                    var popupTemplate = {
                                        title: "ICAO: <b>{Icao}</b>",
                                       content: "<b>{DateStr}</b>. Callsign: <b>{Callsign}</b><br />"
                                        + "Kinh độ: <b>{Lon}</b>. Vĩ độ: <b>{Lat}</b>. Tốc độ: <b>{Speed}</b>. Hướng: <b>{Track}</b>."
                             
                                    };

                                    var pointGraphic = new Graphic({
                                        geometry: point,
                                        symbol: simpleMarkerSymbol,
                                        attributes: attributes,
                                        popupTemplate: popupTemplate
                                    });
                                    listPath[i] = [lon, lat];
                                    graphicsLayer.add(pointGraphic);
                                   

                                });
                                var simpleLineSymbol = {
                                    type: "simple-line",
                                    color: "#008ae6", // orange
                                    width: 2,
                                    style: "short-dash-dot",
                                };

                                var polyline = {
                                    type: "polyline",
                                    paths: listPath
                                };

                                var polylineGraphic = new Graphic({
                                    geometry: polyline,
                                    symbol: simpleLineSymbol
                                });

                                graphicsLayer.add(polylineGraphic);
                            });

                            

     
                            $('.table-map tbody tr').each(function () {
                                $(this).on("click", function () {
                                    var lon = $(this).find('.Longitude').text();
                                    var lat = $(this).find('.Latitude').text();
                                    var alti = $(this).find('.Altitude').text();
                                    var icao = $(this).find('.Icao').text();
                                    var speed = $(this).find('.Speed').text();
                                    var track = $(this).find('.Track').text();
                                    var dateString = $(this).find('.dateString').text();
                                    var callsign = $(this).find('.Callsign').text();
                                    var vertical = $(this).find('.VerticalRate').text();

                                    var point1 = {
                                        type: "point",
                                        longitude: lon,
                                        latitude: lat
                                    };
                                    view.popup.open({
                                        title: "ICAO: <b>"+icao+"</b>",
                                        location: point1,
                                        content: "<span style='font-size : 14px;'><b>" + dateString + "</b>. Callsign: <b>" + callsign + "</b><br />"
                                            + "Vĩ độ: <b>" + lat + "</b>.Kinh độ: <b>" + lon + "</b>."
                                            + " Tốc độ: <b>" + speed + "</b>. Hướng: <b>" + track + "</b></span>"
                                    });
                                });
                            });


                        },
                        error: function (ex) {
                            var r = jQuery.parseJSON(response.responseText);
                            //alert("Message: " + r.Message);
                            console.log(r.Message);
                        }
                    });

                });


                });

            // Create a point
        });
    </script>


    <style>
        html, body {
            padding: 0;
            margin: 0;
            height: 98%;
            width: 100%;
        }

        .main-padding {
            padding: 15px;
        }

        .viewDiv {
            height: 98%;
        }

        .title {
            background-color: #343a40;
            height: 60px;
            text-align: left;
            color: #fff;
            font-size: 25px;
            padding-top: 11px;
            margin-bottom: 0;
            border-bottom: 1px solid gray;
        }

        #viewDiv {
            height: 100%;
            width: 100%;
            padding: 0;
            margin: 0;
        }

        .mainMap {
            height: calc(100% - 60px);
            display: block;
        }
    </style>
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <p class="title">
                <span class="title-container">
                    <span class="glyphicon glyphicon-plane"></span> <span class="flight-text">Flight Map</span>
                </span>
            </p>
        </div>
    </div>

    <div class="main-padding">

        <div class="row">
            <div class="col-md-12 hidden">
                <div class="left-col" style="margin-bottom: 15px; ">
                    <div>
                        <button class="btn btn-primary" id="btnLoad"><span class="glyphicon glyphicon-play"></span> Load Data</button>
                        <button class="btn btn-danger disabled" id="btnStop"><span class="glyphicon glyphicon-stop"></span> Stop loading</button>
                        <input type="text" class="form-control" id="loadTime" value="10" placeholder="Loading time" style="display: inline-block; width:120px; ">
                        <span>Reload data after: <b id="counting">10</b> seconds</span>
                    </div>
                    
                    <!-- Load data after search -->
             
                </div>

            </div>
            <div class="col-md-12">
                <div class="mainMap">
                    <div id="viewDiv"></div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="section-panel search-panel">
                    <div class="section-panel">
                        <header class="panel-heading">
                            <h2 class="panel-title">Tra cứu</h2>
                            @*<p class="panel-subtitle">Tìm kiếm thông tin</p>*@
                        </header>
                        <div class="panel-body">
                            <form class="form-horizontal form-bordered" method="post" style="margin-bottom : 0;">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                           
                                            <label class="col-md-12 control-label" for="inputDefault">ICAO</label>
                                            <div class="col-md-12">
                                                <input type="text" class="form-control" id="inputICAO" />
                                                <div class="form-group" style="margin-bottom : 0; padding-bottom : 0;">
                                                    <div class="col-md-12">
                                                        @*<button class="btn btn-primary" id="btnSearch" style="width : 100%"><span class="glyphicon glyphicon-search"></span> Tìm kiếm </button>*@
                                                        <input type="button" class="btn btn-primary btnSearch" style="width : 100%" value="Tìm kiếm" />
                                                    </div>

                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-12 control-label">Khoảng ngày</label>
                                            <div class="col-md-12">
                                                <div class="input-daterange input-group" data-plugin-datepicker>
                                                    @*<input id="datepicker1" type="date" class="date form-control" name="start">*@
                                                    @Html.TextBox("datepicker1", null, new { id = "datepicker1", @class = "date form-control", @type = "date" })
                                                    <input type="time" class="form-control timeinput time1" id="timepicker1" name="appt">
                                                    <span class="input-group-addon">to</span>
                                                   
                                                    @Html.TextBox("datepicker2", null, new { id = "datepicker2", @class = "date form-control", @type = "date" })
                                                    <input type="time" class="form-control timeinput time2" id="timepicker2" name="appt">
                                                </div>
                                                
                                            </div>

                                            
                                        </div>
                                    </div>
                                </div>
                                
                                
                                
                            </form>

                        </div>
                    </div>
                    
                </div>

            </div>
            <div class="col-md-12">
                <button style="margin-top : 15px;" class="btn btn-primary" id="btnShowMap"><span class="glyphicon glyphicon-plane"></span> Load bản đồ</button>
            </div>
            <div class="col-md-12">
                <table class="table table-map table-striped" style="margin-top: 15px;">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">Ngày giờ</th>
                            <th scope="col">ICAO</th>
                            <th scope="col">Hô hiệu</th>
                            <th scope="col">Kinh độ/Vĩ độ</th>
                            <th scope="col">Tốc độ bay</th>
                            <th scope="col">Độ cao/Hướng</th>
                            <th scope="col">Tốc độ dọc</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>


</body>
</html> 